<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <include file="struts-default.xml"/>
    <package name="com.action" extends="struts-default">
        <action name="UserLogin" class="com.action.UserLogin">
            <result name="success">/init.jsp</result>
            <result name="error">/index.html</result>
        </action>
         <action name="UserRegis" class="com.action.UserRegis">
            <result name="success">/init.jsp</result>
            <result name="error">/index.html</result>
        </action>
    </package>
</struts>


