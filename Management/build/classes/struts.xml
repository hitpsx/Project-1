<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <include file="struts-default.xml"/>
    <package name="com.action" extends="struts-default">
        <action name="UserLogin" class="com.action.UserLogin" method="execute">
            <result name="success">/Home.jsp</result>
            <result name="admin">/HomeAdmin.jsp</result>
            <result name="error">/LoginError.jsp</result>
        </action>
        <action name="Home" class="com.action.Home" method="execute">
            <result name="success">/Home.jsp</result>
            <result name="error">/404.html</result>
    	</action>
        <action name="UserRegis" class="com.action.UserRegis" method="execute">
            <result name="success">/index.html</result>
            <result name="error">/init.jsp</result>
        </action>
        <action name="Profile" class="com.action.Profile" method="execute">
            <result name="success">/profile.jsp</result>
            <result name="error">/init.jsp</result>
    	</action>
    	<action name="SelectHome" class="com.action.SelectHome" method="execute">
            <result name="success">/HomeEquManage.jsp</result>
            <result name="error">/init.jsp</result>
    	</action>
    	 <action name="CsSelect" class="com.action.CsSelect" method="execute">
            <result name="success">/EquInformation.jsp</result>
            <result name="error">/init.jsp</result>
    	</action>
    	<action name="LendButton" class="com.action.LendButton" method="execute">
            <result name="success">/UserLendApplication.jsp</result>
            <result name="error">/init.jsp</result>
    	</action>
    	<action name="LendManage" class="com.action.LendManage" method="execute">
            <result name="success">/Home.jsp</result>
            <result name="error">/404.html</result>
    	</action>
    	<action name="RetirButton" class="com.action.RetirButton" method="execute">
            <result name="success">/UserRetireApplication.jsp</result>
            <result name="error">/404.html</result>
    	</action>
    	<action name="RetireManage" class="com.action.RetireManage" method="execute">
            <result name="success">/Home.jsp</result>
            <result name="error">/404.html</result>
    	</action>
    	<action name="HomeLend" class="com.action.HomeLend" method="execute">
            <result name="success">/UserLendResult.jsp</result>
            <result name="error">/404.html</result>
    	</action>
    	<action name="HomeRetire" class="com.action.HomeRetire" method="execute">
            <result name="success">/UserRetireResult.jsp</result>
            <result name="admin">/AdminRetireResult.jsp</result>
            <result name="error">/404.html</result>
    	</action>
    	<action name="HomeSelect" class="com.action.HomeSelect" method="execute">
            <result name="success">/UserSelectResult.jsp</result>
            <result name="admin">/UserSelectResult.jsp</result>
            <result name="error">/404.html</result>
    	</action>
    	<action name="CsUpdate" class="com.action.CsUpdate" method="execute">
            <result name="success">/UserUpdateCs.jsp</result>
            <result name="error">/404.html</result>
    	</action>
    	<action name="UpdateCs" class="com.action.UpdateCs" method="execute">
            <result name="success">/Home.jsp</result>
            <result name="error">/404.html</result>
    	</action>
    	
    	<action name="ComputerHome" class="com.action.ComputerHome" method="execute">
            <result name="success">/UserSelectResult.jsp</result>
            <result name="error">/404.html</result>
    	</action>
    	
    	<action name="graph" class="com.action.graph" method="execute">
            <result name="success">/graphs.jsp</result>
            <result name="error">/404.html</result>
    	</action>
    	 
    	
    	<action name="view" class="com.action.view" method="execute">
            <result name="success">/AdminViewRetire.jsp</result>
            <result name="error">/404.html</result>
    	</action>
    	
    	<action name="Agree" class="com.action.Agree" method="execute">
            <result name="success">/AdminHandleRetire.jsp</result>
            <result name="error">/404.html</result>
    	</action>
    	
    	<action name="DisAgreeRetire" class="com.action.DisAgreeRetire" method="execute">
            <result name="success">/AdminHandleRetire.jsp</result>
            <result name="error">/404.html</result>
    	</action>
    	
    	<action name="AdminRead" class="com.action.AdminRead" method="execute">
            <result name="success">/AdminRetireResult.jsp</result>
            <result name="error">/404.html</result>
    	</action>
    </package>
</struts>


